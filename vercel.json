{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/node" },
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "dist/Employee-App/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/server.js" },
    { "src": "/(.*)", "dest": "/dist/Employee-App/$1" }
  ]
}
